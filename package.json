{
  "name": "fastify-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Node.js boilerplate using fastify & TypeScript",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc && node resolve-module-alias.js",
    "start": "node build",
    "dev": "nodemon | pino-pretty --colorize",
    "pretest": "npm run build",
    "test": "jest --config .jest.json --coverage",
    "test:watch": "jest --config .jest.json --watch --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonathan06/fastify-typescript-boilerplate.git"
  },
  "keywords": ["nodejs", "fastify", "typescript"],
  "author": "Yonatan Bendahan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yonathan06/fastify-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/yonathan06/fastify-typescript-boilerplate#readme",
  "devDependencies": {
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "pino-pretty": "^4.1.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "env-schema": "^1.1.0",
    "fastify": "^3.3.0",
    "fastify-now": "^2.2.0",
    "fluent-schema": "^1.0.4",
    "http-errors": "^1.8.0",
    "node-fetch": "^2.6.0"
  }
}
